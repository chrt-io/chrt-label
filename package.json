{
  "name": "chrt-label",
  "version": "0.0.40",
  "description": "Label for XY charts component for Chrt",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/chrt-label.min.js",
  "unpkg": "dist/chrt-label.min.js",
  "exports": {
    "umd": "./dist/chrt-label.min.js",
    "default": "./dist/chrt-label.esm.js"
  },
  "keywords": ["chrt", "charts", "labels", "visualization"],
  "watch": {
    "build": {
      "patterns": ["src"],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "postversion": "rollup -c",
    "lint": "eslint src --fix",
    "watch": "npm-watch",
    "dev": "budo test/chrts/index.js:bundle.js --live --dir test/chrts  -- -t [ babelify --extensions .js --global true ]",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/chrt/chrt-label.git"
  },
  "author": "chrt",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/chrt/chrt-label/issues"
  },
  "homepage": "chrt.io",
  "dependencies": {
    "chrt-object": "0.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "babel-plugin-root-import": "^6.6.0",
    "babelify": "^10.0.0",
    "budo": "^11.8.4",
    "chrt": "0.0.157",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-watch": "^0.11.0",
    "rollup": "4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "watch": "^1.0.2"
  },
  "files": ["dist/**/*.js", "src/**/*.js"]
}
